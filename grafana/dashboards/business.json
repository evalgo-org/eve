{
  "dashboard": {
    "title": "EVE Tracing - Business Workflow Dashboard",
    "tags": ["eve", "tracing", "business", "workflows"],
    "timezone": "utc",
    "schemaVersion": 36,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "title": "Workflow Success Rate",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "sum(rate(eve_tracing_workflows_total{status=\"completed\"}[5m])) / sum(rate(eve_tracing_workflows_total[5m]))",
            "legendFormat": "Success Rate",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {"calcs": ["lastNotNull"]},
          "orientation": "auto",
          "textMode": "value_and_name",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 0.95, "color": "yellow"},
                {"value": 0.99, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "title": "Workflows Completed (5m)",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0},
        "targets": [
          {
            "expr": "sum(increase(eve_tracing_workflows_total[5m]))",
            "legendFormat": "Total",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {"calcs": ["lastNotNull"]},
          "orientation": "auto",
          "textMode": "value_and_name",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "title": "Active Workflows",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "sum(eve_tracing_workflows_in_flight)",
            "legendFormat": "In Flight",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {"calcs": ["lastNotNull"]},
          "orientation": "auto",
          "textMode": "value_and_name",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 50, "color": "yellow"},
                {"value": 100, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "title": "Average Workflow Duration",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0},
        "targets": [
          {
            "expr": "sum(rate(eve_tracing_workflow_duration_seconds_sum[5m])) / sum(rate(eve_tracing_workflow_duration_seconds_count[5m]))",
            "legendFormat": "Avg Duration",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {"calcs": ["lastNotNull"]},
          "orientation": "auto",
          "textMode": "value_and_name",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 30, "color": "yellow"},
                {"value": 120, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "title": "Workflow Completion Rate Over Time",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "sum(rate(eve_tracing_workflows_total{status=\"completed\"}[5m]))",
            "legendFormat": "Completed",
            "refId": "A"
          },
          {
            "expr": "sum(rate(eve_tracing_workflows_total{status=\"failed\"}[5m]))",
            "legendFormat": "Failed",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Workflows/sec"},
          {"format": "short"}
        ]
      },
      {
        "title": "Workflow Duration Percentiles",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(eve_tracing_workflow_duration_seconds_bucket[5m])) by (le))",
            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(eve_tracing_workflow_duration_seconds_bucket[5m])) by (le))",
            "legendFormat": "p95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(eve_tracing_workflow_duration_seconds_bucket[5m])) by (le))",
            "legendFormat": "p99",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"},
          {"format": "short"}
        ]
      },
      {
        "title": "Actions by Type (Rate)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "sum(rate(eve_tracing_actions_total[5m])) by (action_type)",
            "legendFormat": "{{action_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Actions/sec"},
          {"format": "short"}
        ]
      },
      {
        "title": "Actions by Object Type (Rate)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "sum(rate(eve_tracing_actions_total[5m])) by (object_type)",
            "legendFormat": "{{object_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Actions/sec"},
          {"format": "short"}
        ]
      },
      {
        "title": "Workflow Step Duration by Action Type",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(eve_tracing_workflow_step_duration_seconds_bucket[5m])) by (le, action_type))",
            "legendFormat": "{{action_type}} p95",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"},
          {"format": "short"}
        ]
      },
      {
        "title": "Top 10 Slowest Workflows (Last Hour)",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
        "targets": [
          {
            "expr": "topk(10, max_over_time(eve_tracing_workflow_duration_seconds[1h]))",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ]
      },
      {
        "title": "Service Throughput (Actions/sec)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 32},
        "targets": [
          {
            "expr": "sum(rate(eve_tracing_actions_total[5m])) by (service_id)",
            "legendFormat": "{{service_id}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Actions/sec"},
          {"format": "short"}
        ]
      },
      {
        "title": "Error Rate by Service",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 32},
        "targets": [
          {
            "expr": "sum(rate(eve_tracing_action_errors_total[5m])) by (service_id)",
            "legendFormat": "{{service_id}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Errors/sec"},
          {"format": "short"}
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [10], "type": "gt"},
              "query": {"params": ["A", "5m", "now"]},
              "type": "query"
            }
          ],
          "name": "High Business Error Rate"
        }
      },
      {
        "title": "Action Success Rate by Type",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 40},
        "targets": [
          {
            "expr": "sum(rate(eve_tracing_actions_total{status=\"completed\"}[5m])) by (action_type) / sum(rate(eve_tracing_actions_total[5m])) by (action_type)",
            "legendFormat": "{{action_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percentunit", "label": "Success Rate", "min": 0, "max": 1},
          {"format": "short"}
        ]
      },
      {
        "title": "Business SLO: 95% of workflows < 60s",
        "type": "gauge",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 40},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(eve_tracing_workflow_duration_seconds_bucket[5m])) by (le))",
            "refId": "A"
          }
        ],
        "options": {
          "showThresholdLabels": true,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "min": 0,
            "max": 120,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 60, "color": "yellow"},
                {"value": 90, "color": "red"}
              ]
            }
          }
        }
      }
    ]
  }
}
