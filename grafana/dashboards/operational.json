{
  "title": "EVE Tracing - Operational Dashboard",
  "tags": [
    "eve",
    "tracing",
    "operational"
  ],
  "timezone": "utc",
  "schemaVersion": 36,
  "version": 1,
  "refresh": "30s",
  "panels": [
    {
      "title": "Action Requests per Second",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(rate(eve_tracing_actions_total[5m])) by (service_id)",
          "legendFormat": "{{service_id}}",
          "refId": "A"
        }
      ],
      "yaxes": [
        {
          "format": "reqps",
          "label": "Requests/sec"
        },
        {
          "format": "short"
        }
      ]
    },
    {
      "title": "Action Error Rate",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(rate(eve_tracing_action_errors_total[5m])) by (service_id)",
          "legendFormat": "{{service_id}}",
          "refId": "A"
        }
      ],
      "yaxes": [
        {
          "format": "reqps",
          "label": "Errors/sec"
        },
        {
          "format": "short"
        }
      ],
      "alert": {
        "conditions": [
          {
            "evaluator": {
              "params": [
                5
              ],
              "type": "gt"
            },
            "query": {
              "params": [
                "A",
                "5m",
                "now"
              ]
            },
            "type": "query"
          }
        ],
        "name": "High Error Rate"
      }
    },
    {
      "title": "Action Latency (p95)",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(eve_tracing_action_duration_seconds_bucket[5m])) by (le, action_type, service_id))",
          "legendFormat": "{{service_id}} - {{action_type}}",
          "refId": "A"
        }
      ],
      "yaxes": [
        {
          "format": "s",
          "label": "Latency"
        },
        {
          "format": "short"
        }
      ]
    },
    {
      "title": "Action Latency (p50, p95, p99)",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(eve_tracing_action_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(eve_tracing_action_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(eve_tracing_action_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ],
      "yaxes": [
        {
          "format": "s",
          "label": "Latency"
        },
        {
          "format": "short"
        }
      ]
    },
    {
      "title": "Async Exporter Queue Size",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 16
      },
      "targets": [
        {
          "expr": "eve_tracing_exporter_queue_size",
          "legendFormat": "Queue Size",
          "refId": "A"
        }
      ],
      "yaxes": [
        {
          "format": "short",
          "label": "Traces"
        },
        {
          "format": "short"
        }
      ],
      "alert": {
        "conditions": [
          {
            "evaluator": {
              "params": [
                8000
              ],
              "type": "gt"
            },
            "query": {
              "params": [
                "A",
                "5m",
                "now"
              ]
            },
            "type": "query"
          }
        ],
        "name": "Queue Near Capacity"
      }
    },
    {
      "title": "Async Exporter Dropped Traces",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 16
      },
      "targets": [
        {
          "expr": "sum(rate(eve_tracing_exporter_queue_dropped_total[5m])) by (service_id)",
          "legendFormat": "{{service_id}}",
          "refId": "A"
        }
      ],
      "yaxes": [
        {
          "format": "reqps",
          "label": "Dropped/sec"
        },
        {
          "format": "short"
        }
      ],
      "alert": {
        "conditions": [
          {
            "evaluator": {
              "params": [
                1
              ],
              "type": "gt"
            },
            "query": {
              "params": [
                "A",
                "5m",
                "now"
              ]
            },
            "type": "query"
          }
        ],
        "name": "Traces Being Dropped"
      }
    },
    {
      "title": "Exporter Batch Latency",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 24
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(eve_tracing_exporter_latency_seconds_bucket[5m]))",
          "legendFormat": "p95",
          "refId": "A"
        }
      ],
      "yaxes": [
        {
          "format": "s",
          "label": "Latency"
        },
        {
          "format": "short"
        }
      ]
    },
    {
      "title": "Exporter Batch Success Rate",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 24
      },
      "targets": [
        {
          "expr": "sum(rate(eve_tracing_exporter_batches_total{status=\"success\"}[5m])) / sum(rate(eve_tracing_exporter_batches_total[5m]))",
          "legendFormat": "Success Rate",
          "refId": "A"
        }
      ],
      "yaxes": [
        {
          "format": "percentunit",
          "label": "Success Rate"
        },
        {
          "format": "short"
        }
      ]
    },
    {
      "title": "PostgreSQL Write Rate",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 32
      },
      "targets": [
        {
          "expr": "sum(rate(eve_tracing_postgresql_writes_total{status=\"success\"}[5m])) by (table)",
          "legendFormat": "{{table}}",
          "refId": "A"
        }
      ],
      "yaxes": [
        {
          "format": "wps",
          "label": "Writes/sec"
        },
        {
          "format": "short"
        }
      ]
    },
    {
      "title": "PostgreSQL Errors",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 32
      },
      "targets": [
        {
          "expr": "sum(rate(eve_tracing_postgresql_errors_total[5m])) by (operation, error_type)",
          "legendFormat": "{{operation}} - {{error_type}}",
          "refId": "A"
        }
      ],
      "yaxes": [
        {
          "format": "reqps",
          "label": "Errors/sec"
        },
        {
          "format": "short"
        }
      ]
    },
    {
      "title": "S3 Upload Rate",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 40
      },
      "targets": [
        {
          "expr": "sum(rate(eve_tracing_s3_uploads_total{status=\"success\"}[5m])) by (bucket)",
          "legendFormat": "{{bucket}}",
          "refId": "A"
        }
      ],
      "yaxes": [
        {
          "format": "reqps",
          "label": "Uploads/sec"
        },
        {
          "format": "short"
        }
      ]
    },
    {
      "title": "S3 Errors",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 40
      },
      "targets": [
        {
          "expr": "sum(rate(eve_tracing_s3_errors_total[5m])) by (operation, error_type)",
          "legendFormat": "{{operation}} - {{error_type}}",
          "refId": "A"
        }
      ],
      "yaxes": [
        {
          "format": "reqps",
          "label": "Errors/sec"
        },
        {
          "format": "short"
        }
      ]
    },
    {
      "title": "Trace Payload Sizes",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 48
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(eve_tracing_trace_payload_bytes_bucket[5m])) by (le, type))",
          "legendFormat": "{{type}} p95",
          "refId": "A"
        }
      ],
      "yaxes": [
        {
          "format": "bytes",
          "label": "Size"
        },
        {
          "format": "short"
        }
      ]
    },
    {
      "title": "OpenTelemetry Trace Links",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 48
      },
      "targets": [
        {
          "expr": "sum(rate(eve_tracing_otel_trace_links_total[5m])) by (service_id)",
          "legendFormat": "{{service_id}}",
          "refId": "A"
        }
      ],
      "yaxes": [
        {
          "format": "reqps",
          "label": "Links/sec"
        },
        {
          "format": "short"
        }
      ]
    }
  ]
}
